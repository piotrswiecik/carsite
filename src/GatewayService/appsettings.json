{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "auctions-route": {
        "ClusterId": "auctions-cluster",
        "Match": {
          "Path": "/auctions/{**catchall}"
        },
        "Transforms": [
          {
            "PathPattern": "/api/auctions/{**catchall}"
          }
        ]
      },
      "search-route": {
        "ClusterId": "search-cluster",
        "Match": {
          "Path": "/search/{**catchall}",
          "Methods": [
            "GET"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "/api/search/{**catchall}"
          }
        ]
      }
    },
    "Clusters": {
      "auctions-cluster": {
        "Destinations": {
          "auctions-cluster/api": {
            "Address": "http://localhost:8000"
          }
        }
      },
      "search-cluster": {
        "Destinations": {
          "search-cluster/api": {
            "Address": "http://localhost:8001"
          }
        }
      }
    }
  }
}
